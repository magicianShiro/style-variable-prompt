{
	"name": "style-variable-prompt",
	"displayName": "Style Variable Prompt",
	"description": "在输入的时候提示css预处理器的变量",
	"keywords": [
		"scss",
		"less",
		"scss variable",
		"less variable",
		"style variable",
		"variable prompt",
		"style variable prompt"
	],
	"publisher": "navi",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/magicianShiro/style-variable-prompt"
	},
	"homepage": "https://github.com/magicianShiro/style-variable-prompt",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:less",
		"onLanguage:scss"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "styleVariablePrompt configure",
			"properties": {
				"styleVariablePrompt.variableFilePath": {
					"type": "array",
					"description": "变量的路径"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"patch": "npm version patch && git push origin master"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.6",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
